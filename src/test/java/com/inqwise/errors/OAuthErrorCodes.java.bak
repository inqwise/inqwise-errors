package com.inqwise.errors;

import com.fasterxml.jackson.annotation.JsonValue;

/**
 * OAuth 2.0 error codes as defined in RFC 6749 Section 5.2.
 * 
 * @see &lt;a href="https://tools.ietf.org/html/rfc6749#section-5.2"&gt;RFC 6749 Section 5.2&lt;/a&gt;
 */
public enum OAuthErrorCodes implements ErrorCode {
    InvalidRequest("invalid_request"),
    UnauthorizedClient("unauthorized_client"),
    AccessDenied("access_denied"),
    UnsupportedResponseType("unsupported_response_type"),
    InvalidScope("invalid_scope"),
    ServerError("server_error"),
    TemporarilyUnavailable("temporarily_unavailable"),
    InvalidClient("invalid_client"),
    InvalidToken("invalid_token");

    private final String code;

    OAuthErrorCodes(String code) {
        this.code = code;
    }

@JsonValue
    @Override
    public String toString() {
        return code;
    }

    @Override
    public String group() {
        return "oauth";
